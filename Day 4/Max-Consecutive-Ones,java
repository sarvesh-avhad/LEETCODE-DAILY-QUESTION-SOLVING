class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int sum = 0;
        int current = 0;
        for(int i=0;i<nums.length;i++){
            if(nums[i] != 1){
                sum = Math.max(current,sum);
                current = 0;
            }
            else{
                current++;
            }
        }
    return Math.max(current,sum);
    }
}
